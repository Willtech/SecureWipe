echo first pass.
sudo dd if=/dev/urandom of=/dev/sdb bs=1M status=progress
echo first pass..
sudo dd if=/dev/urandom of=/dev/sdb bs=1M status=progress
echo first pass...
sudo dd if=/dev/urandom of=/dev/sdb bs=1M status=progress
echo first pass....
sudo dd if=/dev/urandom of=/dev/sdb bs=1M status=progress
echo first pass.....
sudo dd if=/dev/urandom of=/dev/sdb bs=1M status=progress
echo first pass......
sudo dd if=/dev/urandom of=/dev/sdb bs=1M status=progress
echo first pass.......
sudo dd if=/dev/urandom of=/dev/sdb bs=1M status=progress
exit


